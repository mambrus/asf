<asf xmlversion="1.0">
	<project id="thirdparty.wireless.bt_wilc_sdk.le_wifi_prov.samv71q21_samv71_xplained_ultra" caption="LE Wifi Provisioning Example for SAMV71 XPlained Ultra">
		<!-- Source -->
		<info type="description" value="summary">
			LE Wifi Provisioning Service Application.
		</info>	
		
		<!-- Header File -->
		<build type="include-path" value="."/>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clock.h"/>
		<build type="header-file" value="conf_sd_mmc.h"/>
		<build type="header-file" value="conf_tinyservices.h"/>
		<build type="header-file" value="conf_uart_serial.h"/>
		<build type="header-file" value="conf_wilc.h"/>
