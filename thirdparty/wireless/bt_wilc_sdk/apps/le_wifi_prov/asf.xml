<asf xmlversion="1.0">
	<module type="application" caption="Le Wifi Provisioning Service Application" id="thirdparty.wireless.bt_wilc_sdk.apps.le_wifi_prov">
		<!-- Summary Description -->
		<info type="description" value="summary">
			Le Wifi Provisioning Service Application for Wilc3000
		</info>
		
		<!-- Header File -->
		<build type="include-path" value="."/>
		<build type="header-file" value="le_prov.h"/>
		<build type="header-file" value="le_prov_db.h"/>
		<build type="header-file" value="wifi_prov.h"/>
				
		<!-- Source File -->
		<build type="c-source" value="main.c"/>
		<build type="c-source" value="le_prov.c"/>
		<build type="c-source" value="wifi_prov.c"/>
		
