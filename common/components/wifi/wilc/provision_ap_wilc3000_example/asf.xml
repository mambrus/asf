<asf xmlversion="1.0">
	<select-by-device id="common.components.wifi.wilc3000.provision_ap_example">
		<module type="application" id="common.components.wifi.wilc3000.provision_ap_example#1" caption="WILC3000 Provision AP Example">
			<!-- Summary Description -->
			<info type="description" value="summary">
				This example demonstrates how to Provision an Access Point (AP) using the WILC3000 Wi-Fi module.
			</info>

			<!-- Source -->
			<build type="include-path" value="."/>
			<build type="c-source" value="provision_ap.c"/>
			<build type="header-file" value="provision_ap.h"/>
			<build type="c-source" value="main.c"/>
			<build type="include-path" value="script"/>
			<build type="distribute" subtype="file" value="script/tcpclient_for_ap_provisioning_test.py"/>
			<build type="include-path" value="android_app"/>
			<build type="distribute" subtype="file" value="android_app/provision_ap_app.apk"/>
			<build type="distribute" subtype="file" value="android_app/provision_ap_quick_start_guide.docx"/>
			
			<!-- Device support -->
			<device-support value="sam4s"/>
			<device-support value="samv71"/>
			
			<!-- Required modules -->
			<require idref="common.components.wifi.wilc3000"/>
			<require idref="common.services.basic.clock"/>
			<require idref="common.utils.osprintf"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
	</select-by-device>
</asf>
