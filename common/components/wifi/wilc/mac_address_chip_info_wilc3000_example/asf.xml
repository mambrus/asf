<asf xmlversion="1.0">
	<select-by-device id="common.components.wifi.wilc3000.mac_address_chip_info_example">
		<module type="application" id="common.components.wifi.wilc3000.mac_address_chip_info_example#1" caption="WILC3000 MAC Address and Chip Info Example">
			<!-- Summary Description -->
			<info type="description" value="summary">
				This example demonstrates how to retrieve the MAC address and Chip Info of the WILC1000 Wi-Fi module.
			</info>

			<!-- Source -->
			<build type="include-path" value="."/>
			<build type="c-source" value="mac_address_chip_info.c"/>
			<build type="header-file" value="mac_address_chip_info.h"/>
			<build type="c-source" value="main.c"/>

			<!-- Device support -->
			<device-support value="sam4s"/>
			<device-support value="samv71"/>
			
			<!-- Required modules -->
			<require idref="common.components.wifi.wilc3000"/>
			<require idref="common.services.basic.clock"/>
			<require idref="common.utils.osprintf"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
	</select-by-device>
</asf>
