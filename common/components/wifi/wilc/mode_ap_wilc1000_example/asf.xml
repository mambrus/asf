<asf xmlversion="1.0">
	<select-by-device id="common.components.wifi.wilc.mode_ap_wilc1000_example">
		<module type="application" id="common.components.wifi.wilc.mode_ap_wilc1000_example#1" caption="WILC1000 Mode AP Example">
			<!-- Summary Description -->
			<info type="description" value="summary">
				This example demonstrates how to enter Access Point (AP) mode using the WILC1000 Wi-Fi module.
			</info>

			<!-- Source -->
			<build type="include-path" value="."/>
			<build type="c-source" value="main.c"/>
			<build type="c-source" value="ap.c"/>
			<build type="header-file" value="ap.h"/>

			<!-- Device support -->
			<device-support value="sam4s"/>

			<!-- Required modules -->
			<require idref="common.components.wifi.wilc1000"/>
			<require idref="common.services.basic.clock"/>
			<require idref="common.utils.osprintf"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
	</select-by-device>
</asf>
