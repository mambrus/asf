<asf xmlversion="1.0">
	<module type="application" id="sam.applications.tc_qdec_example" caption="SAM4E TC Quadrature Decoder Example">
		<info type="description" value="summary">
		Example application for SAM4E TC module demonstrating TC Quadrature Decoder features like position and 
		Rotation measurement, Speed measurement, Direction status and change detection.
		</info>
		<info type="keyword" value="technology">
			<keyword value="Timers"/>
		</info>
		<info type="gui-flag" value="move-to-root"/>
		<build type="c-source" value="tc_qdec_example.c"/>
		<build type="include-path" value="."/>
		<device-support value="sam4e"/>
		<require idref="sam.drivers.tc"/>
		<require idref="sam.drivers.wdt"/>
		<require idref="common.services.basic.gpio"/>
		<require idref="common.services.ioport"/>
		<require idref="common.services.basic.clock"/>
		<require idref="common.utils.stdio.stdio_serial"/>
		<require idref="common.services.delay"/>
	</module>
</asf>
